# Duplicate check using MODEL_SCORE_ID
if "MODEL_SCORE_ID" in df.columns:
    dup_msid = df.duplicated(subset=["MODEL_SCORE_ID"], keep=False)

    print("=== Duplicate MODEL_SCORE_ID check ===")
    print("Duplicate rows:", int(dup_msid.sum()))
    print("Duplicate pct:", round(float(dup_msid.mean() * 100), 4))

    # Optional: number of unique MODEL_SCORE_IDs
    print("Total rows:", len(df))
    print("Unique MODEL_SCORE_IDs:", df["MODEL_SCORE_ID"].nunique())
else:
    print("MODEL_SCORE_ID not present in df")
